{
  "expo": {
    "name": "Pasco Jobs",
    "slug": "frontend2",
    "icon": "./assets/main/icon.png",
    
    "primaryColor": "#FFFFFF",
    "version": "2.2.0",     
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/main/splash.png",
      "resizeMode": "cover" ,
      "backgroundColor": "#262159"      
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
    
      "adaptiveIcon": {
        "foregroundImage": "./assets/main/adaptive.png",
        "backgroundColor": "#262159" 
        
      },
      "icon": "./assets/main/icon.png",
      
      "package": "com.godoyliam.pascojobs"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e8935407-5fb2-489e-bed7-e709bd3549d0"
      }
    },
    
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]
  }
}
